Fixed Order Quantity=
	                                            1000
	~	
	~		|

Supply Line= INTEG (
	Purchasing-Shipping,
		Demand Forecast*Perceived Lead Time)
	~	
	~		|

Service Policy Coefficient=
	2
	~	
	~		|

Purchase Amount=
	IF THEN ELSE(Supply Line+Inventory <= Reorder Quantity, Fixed Order Quantity, 0)
	~	
	~		|

Reorder Quantity=
	Demand Forecast*Perceived Lead Time+Service Policy Coefficient*Sd Demand*SQRT( Perceived Lead Time\
		)
	~	
	~		|

Inventory= INTEG (
	Shipping-Selling,
		Fixed Order Quantity)
	~	
	~		|

Lead Time Adjustment=
	5
	~	
	~		|

Underage Cost=
	Deficiency*Unit Underage Cost
	~	
	~		|

Cost=
	Underage Cost+Overage Cost
	~	
	~		|

Unit Underage Cost=
	9
	~	
	~		|

Unit Overage Cost=
	1
	~	
	~		|

Mean Demand=
	100
	~	
	~		|

Mean Lead Time=
	5
	~	
	~		|

Std Lead Time=
	3
	~	
	~		|

Overage Cost=
	(Inventory+Supply Line)* Unit Overage Cost
	~	
	~		|

Sd Demand=
	50
	~	
	~		|

Perceived Lead Time=
	SMOOTH(Lead Time, Lead Time Adjustment)
	~	
	~		|

Deficiency=
	MAX(0, Backlog-Selling)
	~	
	~		|

Backlog Out=
	Selling
	~	
	~		|

Backlog= INTEG (
	Backlog In-Backlog Out,
		100)
	~	
	~		|

Selling=
	MIN(Backlog, Inventory)
	~	
	~		|

Backlog In=
	Demand
	~	
	~		|

Purchasing=
	Purchase Amount
	~	
	~		|

Lead Time=
	RANDOM NORMAL( 1, 10, Mean Lead Time, Std Lead Time, 3456)
	~	
	~		|

Shipping=
	Supply Line/Lead Time
	~	
	~		|

Demand=
	RANDOM NORMAL( 0, 200, Mean Demand, Sd Demand, 1111)
	~	
	~		|

Demand Forecast=
	SMOOTH(Demand, Forecaset Period)
	~	
	~		|

Forecaset Period=
	5
